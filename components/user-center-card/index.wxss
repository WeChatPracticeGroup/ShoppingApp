@import "../../style/userCenterIcon.wxss";

.user-center-card {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 480rpx;
  background-size: 100% 480rpx;
  background-repeat: no-repeat;
  background-image: url('data:image/jpeg;base64,');
}

.user-container {
  display: flex;
  flex-direction: column;
  position: fixed;
  width: 100%;
  padding: 0 24rpx;
  margin-top: 100rpx;
}

.user-avatar {
  overflow: hidden;
  display: flex;
  width: 100%;
}

.user-avatar-image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  background-size: 130rpx 130rpx;
  background-repeat: no-repeat;
  margin-right: 30rpx;
  background-color: white;
}

.user-avatar-info {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.user-info {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-top: 50rpx;
}

.user-info-container {
  width: 100%;
  display: flex;
  font-size: 22rpx;
  align-items: center;
}

.user-info-button {
  align-items: center;
  border-radius: unset;
  background-color: transparent;
  box-sizing: none;
  margin: 0;
  line-height: 1.2;
  padding: 0;
  border: 0px;
  color: inherit;
}

.user-info-button::after {
    border: 0px;
}

.user-info-text {
  display: flex;
  flex-direction: column;
  text-align: left;
  font-size: 13px;
  color: #000000;
}

.user-info-icon {
  font-size: 50rpx;
  color: #1296db;
  margin-right: 10rpx;
}